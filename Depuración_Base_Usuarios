    {
      "cell_type": "code",
      "source": [
        "#Aplicar funciones para homologar a nivel general.\n",
        "lista = ['mes', 'sexo', 'fecha nacimiento',\n",
        "       'departamento', 'municipio', 'barrio', 'fecha inicio al pgp',\n",
        "       'fecha primera clase funcional', 'primera clase funcional',\n",
        "       'fecha última clase funcional', 'última clase funcional', 'ciclo_vital',\n",
        "       'quinquenio']\n",
        "for i in lista:\n",
        "    users[i] = users[i].str.lower() #minuscula\n",
        "    users[i] = users[i].str.strip() #espacios\n",
        "    users[i] = users[i].apply(lambda x: normalize(\"NFKD\", x).encode(\"ascii\", \"ignore\").decode(\"ascii\") if pd.notnull(x) else x)"
      ],
      "metadata": {
        "id": "MSHV9g2P9Ssn"
      },
      "execution_count": 10,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
